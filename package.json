{
  "name": "changepassword",
  "version": "0.1.0",
  "private": true,
  "homepage": "/changepassword/",
  "dependencies": {
    "antd": "^4.16.12",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-json-view": "^1.19.1",
    "react-scripts": "3.1.1",
    "typescript": "^4.3.5",
    "universal-cookie": "^4.0.2"
  },
  "scripts": {
    "start": "set PORT=9001 && react-scripts start",
    "build": "react-scripts build && move build changepassword",
    "build:win": "react-scripts build && move build changepassword",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-cmd": "set HTTPS=false && set PORT=9001 && npm start",
    "start-pshell": "($env:HTTPS = \"true\") -and ($env:PORT = \"9000\") -and (npm start)",
    "start-bash": "HTTPS=true PORT=9001 npm start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "https://localhost:9443"
}
